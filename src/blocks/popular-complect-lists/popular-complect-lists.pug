mixin popular-complect-lists(props)

  .popular-complect-lists&attributes(attributes)
    .popular-complect-lists__inner
      .popular-complect-lists__item
        +card-product-popular('img/popular1.jpg', 'Процессор AMD Ryzen 7 2700 BOX', '18 850', '14 999 ₽', 'plus')
