        +card-product-popular('img/popular2.jpg', 'Видеокарта Palit GeForce RTX 2070 Super GAMEROCK', '45 799', '40 799 ₽', 'plus')
      .popular-complect-lists__item
        +card-product-popular('img/popular3.jpg', '480 ГБ SSD-накопитель Intel Optane 905P Series', '48 499', '41 499 ₽', 'arrow')
